{
  "name": "markdown-rambler",
  "version": "0.0.14",
  "description": "Yet another opinionated & powerful static site generator.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": "github:webpro/markdown-rambler",
  "homepage": "https://github.com/webpro/markdown-rambler",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "watch": "tsup src/index.ts --dts --format esm,cjs --watch",
    "test": "uvu -r tsm test",
    "prepublishOnly": "npm test && rm -rf dist && npm run build",
    "release": "release-it"
  },
  "keywords": [
    "static",
    "site",
    "generator",
    "markdown",
    "remark",
    "rehype"
  ],
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl",
    "url": "https://www.webpro.nl"
  },
  "license": "MIT",
  "dependencies": {
    "globby": "^13.1.1",
    "hastscript": "^7.0.2",
    "htm": "^3.1.0",
    "lodash": "^4.17.21",
    "mdast-util-gfm-table": "^1.0.4",
    "micromark-extension-gfm-table": "^1.0.5",
    "minisearch": "^4.0.3",
    "prettier": "^2.6.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-document": "^6.0.1",
    "rehype-format": "^4.0.1",
    "rehype-highlight": "^5.0.2",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "rehype-urls": "^1.1.1",
    "remark-directive": "^2.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-parse": "^10.0.1",
    "remark-prettier": "^1.3.0",
    "remark-reference-links": "^6.0.1",
    "remark-rehype": "^10.1.0",
    "schema-dts": "^1.1.0",
    "svgo": "^2.8.0",
    "to-vfile": "^7.2.3",
    "unified": "^10.1.2",
    "unist-util-is": "^5.1.1",
    "unist-util-remove": "^3.1.0",
    "unist-util-visit": "^4.1.0",
    "vfile-matter": "^3.0.1",
    "vfile-mkdirp": "^3.0.0",
    "vfile-rename": "^2.0.0",
    "xast-util-feed": "^1.2.0",
    "xast-util-to-xml": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "release-it": "^15.0.0",
    "tsm": "^2.2.1",
    "tsup": "^5.12.2",
    "typescript": "^4.6.3",
    "uvu": "^0.5.3"
  },
  "engines": {
    "node": ">=14"
  }
}
